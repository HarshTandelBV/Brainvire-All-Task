<?xml version="1.0" encoding="utf-8"?>

<templates>
    <t t-name="demo_module.ListView.Buttons" t-inherit="web.ListView.Buttons" t-inherit-mode="primary">
        <xpath expr="//div[hasclass('o_list_buttons')]" position="inside">
            <button class="btn btn-secondary" type="button" t-on-click="getData">
                Get Excel Data
            </button>
            <button class="btn btn-secondary" type="button" t-on-click="getOrderSatusData">
                Order Status
            </button>
        </xpath>
    </t>

</templates>